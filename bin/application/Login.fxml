<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Hyperlink?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="560.0" prefWidth="712.0" style="-fx-background-color: #080808;" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Button id="enter" fx:id="enter" layoutX="92.0" layoutY="355.0" mnemonicParsing="false" onMouseClicked="#enter" prefHeight="39.0" prefWidth="103.0" style="-fx-background-color: #ff8b3d; -fx-background-radius: 20;" text="Enter" />
      <TextField id="userInput" fx:id="userInput" layoutX="48.0" layoutY="207.0" prefHeight="39.0" prefWidth="202.0" promptText="username" style="-fx-background-radius: 10;">
         <font>
            <Font name="System Italic" size="13.0" />
         </font></TextField>
      <StackPane layoutX="135.0" layoutY="99.0" />
      <PasswordField id="userPassword" fx:id="userPassword" layoutX="48.0" layoutY="281.0" prefHeight="39.0" prefWidth="202.0" promptText="password" style="-fx-background-radius: 10;">
         <font>
            <Font name="System Italic" size="13.0" />
         </font>
      </PasswordField>
      <Hyperlink fx:id="newaccount" layoutX="88.0" layoutY="415.0" onMouseClicked="#GotoRegister" prefHeight="24.0" prefWidth="140.0" text="Click here to register!" textFill="#a0c8e4" underline="true" />
      <Label layoutX="116.0" layoutY="139.0" prefHeight="39.0" prefWidth="114.0" text="LOGIN" textFill="WHITE">
         <font>
            <Font name="System Bold" size="22.0" />
         </font>
         <effect>
            <Glow />
         </effect>
      </Label>
      <ImageView fitHeight="589.0" fitWidth="375.0" layoutX="339.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../OneDrive/Desktop/images/images.jpeg" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
